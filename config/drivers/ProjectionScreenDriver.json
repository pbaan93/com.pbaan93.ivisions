        "captureLevel": "warn",
        "debounceTime": 500,
        "minTxInterval": 0
    },
    "pair": [
        {
            "id": "imitate",
            "rf_template": "imitate"
        },
        {
            "id": "done",
            "rf_template": "done"
        }
    ]
}